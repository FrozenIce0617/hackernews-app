{
  "name": "hackernews",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:device": "yarn ios --device \"iPhone\"",
    "test": "jest",
    "link:icons": "react-native link react-native-vector-icons",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "husky:post": "yarn && yarn clean && yarn pod"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-vector-icons": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.14.5",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^12.1.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-dotenv-import": "^2.2.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "tslib": "^2.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "tslint-etc": "^1.13.10",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "tsutils": "^3.21.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint",
      "yarn format:ts",
      "git add"
    ],
    "*.md": [
      "yarn format:md",
      "git add"
    ],
    "*.json": [
      "yarn format:json",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn tsc && lint-staged",
      "post-checkout": "yarn husky:post",
      "post-merge": "yarn husky:post"
    }
  }
}
